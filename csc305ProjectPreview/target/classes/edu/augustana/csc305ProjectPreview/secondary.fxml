<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.augustana.csc305ProjectPreview.SecondaryController">
   <center>
      <VBox alignment="CENTER" spacing="20.0">
          <children>
            <Label text="New Project">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Button mnemonicParsing="false" text="Blank Template" />
            <Button mnemonicParsing="false" text="Premade Template" />
              <Button fx:id="secondaryButton" onAction="#switchToPrimary" text="Main Menu" />
          </children>
          <padding>
              <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
          </padding>
      </VBox>
   </center>
</BorderPane>
